    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-oauth/google": "^0.11.1",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/query-sync-storage-persister": "^4.35.3",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-persist-client": "^4.35.5",
    "@types/react-beautiful-dnd": "^13.1.5",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compressorjs": "^1.2.1",
    "crypto-random-string": "^5.0.0",
    "dayjs": "^1.11.10",
    "firebase": "^10.4.0",
    "idb-keyval": "^6.2.1",
    "jotai": "^2.4.2",
    "jotai-location": "^0.5.1",
    "localforage": "^1.10.0",
    "lucide-react": "^0.279.0",
    "next": "^13.5.4",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.1.0",
    "next-sitemap": "^4.2.2",
    "next-themes": "^0.2.1",
    "nextjs-progressbar": "^0.0.16",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-activity": "^2.1.3",
    "react-beautiful-dnd": "^13.1.1",
    "react-bottom-scroll-listener": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.0.2",
    "react-form-hook": "^0.0.1",
    "react-hook-form": "^7.45.4",
    "react-icons": "^4.11.0",
    "react-lines-ellipsis": "^0.15.4",
    "react-linkify": "^1.0.0-alpha",
    "react-mentions": "^4.4.10",
    "react-query": "^3.39.3",
    "react-remove-scroll-bar": "^2.3.4",
    "swiper": "^10.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
